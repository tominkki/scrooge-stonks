    "@types/node-cache": "^4.2.5",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "apollo-boost": "^0.4.9",
    "cross-fetch": "^3.0.6",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@fast-csv/parse": "^4.3.6",
